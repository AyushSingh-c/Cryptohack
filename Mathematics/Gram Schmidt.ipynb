   "source": [
    "import numpy as np\n",
    "\n",
    "def gramschmidt(X):\n",
    "    A=[]\n",
    "    A.append(X[0])\n",
    "    for i in X[1:]:\n",
    "        x = i\n",
    "        for j in A:\n",
    "            x = x - ((np.dot(i,j)/np.dot(j,j))*j)\n",
    "        A.append(x)\n",
    "    return A\n",
    "\n",
    "print(gramschmidt(np.array([[4,1,3,-1],[2,1,-3,4],[1,0,-2,7],[6, 2, 9, -5]])))"
   ]
  }
